<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/j2ee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
		  http://java.sun.com/xml/ns/j2ee/ejb-jar_2_1.xsd"
         version="2.1">
  <enterprise-beans>
      <session>
          <ejb-name>PrintSturctureEJB</ejb-name>
          <home>ru.elleriumsoft.structure.print.printonscreen.PrintStructureHome</home>
          <remote>ru.elleriumsoft.structure.print.printonscreen.PrintStructure</remote>
          <ejb-class>ru.elleriumsoft.structure.print.printonscreen.PrintStructureBean</ejb-class>
          <session-type>Stateless</session-type>
          <transaction-type>Container</transaction-type>
      </session>
      <session>
          <ejb-name>CommandsForStructureEJB</ejb-name>
          <home>ru.elleriumsoft.structure.structurecommands.CommandsForStructureHome</home>
          <remote>ru.elleriumsoft.structure.structurecommands.CommandsForStructure</remote>
          <ejb-class>ru.elleriumsoft.structure.structurecommands.CommandsForStructureBean</ejb-class>
          <session-type>Stateless</session-type>
          <transaction-type>Container</transaction-type>
      </session>
      <session>
          <ejb-name>ActionForStructureEJB</ejb-name>
          <home>ru.elleriumsoft.structure.modifications.ActionForStructureHome</home>
          <remote>ru.elleriumsoft.structure.modifications.ActionForStructure</remote>
          <ejb-class>ru.elleriumsoft.structure.modifications.ActionForStructureBean</ejb-class>
          <session-type>Stateful</session-type>
          <transaction-type>Container</transaction-type>
      </session>
      <session>
          <ejb-name>HandlingOfStatesEJB</ejb-name>
          <home>ru.elleriumsoft.structure.print.handlingofstates.HandlingOfStatesHome</home>
          <remote>ru.elleriumsoft.structure.print.handlingofstates.HandlingOfStates</remote>
          <ejb-class>ru.elleriumsoft.structure.print.handlingofstates.HandlingOfStatesBean</ejb-class>
          <session-type>Stateless</session-type>
          <transaction-type>Container</transaction-type>
      </session>
      <session>
          <ejb-name>ObjectOfStructureEJB</ejb-name>
          <home>ru.elleriumsoft.structure.objectstructure.ObjectOfStructureHome</home>
          <remote>ru.elleriumsoft.structure.objectstructure.ObjectOfStructure</remote>
          <ejb-class>ru.elleriumsoft.structure.objectstructure.ObjectOfStructureBean</ejb-class>
          <session-type>Stateful</session-type>
          <transaction-type>Container</transaction-type>
      </session>
      <session>
          <ejb-name>ObjectDeptEJB</ejb-name>
          <home>ru.elleriumsoft.department.object.ObjectDeptHome</home>
          <remote>ru.elleriumsoft.department.object.ObjectDept</remote>
          <ejb-class>ru.elleriumsoft.department.object.ObjectDeptBean</ejb-class>
          <session-type>Stateful</session-type>
          <transaction-type>Container</transaction-type>
      </session>
      <entity>
          <ejb-name>StructureProcessingFromDbEJB</ejb-name>
          <home>ru.elleriumsoft.structure.entity.StructureProcessingFromDbHome</home>
          <remote>ru.elleriumsoft.structure.entity.StructureProcessingFromDb</remote>
          <ejb-class>ru.elleriumsoft.structure.entity.StructureProcessingFromDbBean</ejb-class>
          <persistence-type>Bean</persistence-type>
          <prim-key-class>java.lang.Integer</prim-key-class>
          <reentrant>false</reentrant>
      </entity>
      <entity>
          <ejb-name>EntityDeptEJB</ejb-name>
          <home>ru.elleriumsoft.department.entity.EntityDeptHome</home>
          <remote>ru.elleriumsoft.department.entity.EntityDept</remote>
          <ejb-class>ru.elleriumsoft.department.entity.EntityDeptBean</ejb-class>
          <persistence-type>Bean</persistence-type>
          <prim-key-class>java.lang.Integer</prim-key-class>
          <reentrant>true</reentrant>
      </entity>
  </enterprise-beans>
</ejb-jar>
